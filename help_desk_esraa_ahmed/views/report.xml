<odoo>
    <record model="ir.actions.act_window" id="action_supply_chain_line_report">
        <field name="name">supply chain detail</field>
        <field name="res_model">supply.chain.line</field>
        <field name="view_mode">pivot,graph</field>
        <field name="help" type="html">
            <p>
               No data
            </p>
        </field>
    </record>

    <menuitem name ="Report"
              id="report_supply_chain"
